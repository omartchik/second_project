<h1>Nouveau restaurant</h1>

<%= form_for @restaurant do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :description, "Description" %>
  <%= f.text_field :description %>
  <%= f.label :address, "Address" %>
  <%= f.text_field :address %>
  <%= f.label :rating, "Rate" %>
  <%= f.number_field :rating %>
  <%= f.submit %>
<% end %>
